  "scripts": {
    "start": "react-native start",
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "bundle": "npx jetify && cd android && ./gradlew bundleRelease && cd ../",
    "release": "npx jetify && cd android && ./gradlew assembleRelease && cd ../",
    "pod": "cd ios && pod install && cd ../",
    "pull": "git pull --rebase",
    "install-debug": "cd android/app/build/outputs/apk/debug && adb install -r ./app-debug.apk",
    "install-release": "cd android/app/build/outputs/apk/release && adb install -r ./app-release.apk",
    "cpr": "./release-app.sh",
    "cpc": "./clear-deploy.sh",
    "test": "jest",
    "lint": "eslint ."
  },
